{
    "version": "0.2.0",
    "configurations":
    [
        {
            "name": "Diov",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Web/bin/Debug/net5.0/Diov.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Web",
            "stopAtEntry": false,
            "serverReadyAction":
            {
                "action": "openExternally"
            },
            "env":
            {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "https://localhost:5001"
            },
            "sourceFileMap":
            {
                "/Views": "${workspaceFolder}/src/Web/Views"
            }
        }
    ]
}